[run]
source = .

[report]
show_missing = False

exclude_lines = 
  except Exception as e:
  if settings.DEBUG
  pragma: no cover

omit =
  */migrations/*
  manage.py
  settings.py
  */pawprints/wsgi.py
  */pawprints/asgi.py
  */log/*
  petitions/management/*
  auth/admin.py
  auth/apps.py
  auth/models.py
  pawprints/settings.py
  pawprints/routing.py
  petitions/routing.py
